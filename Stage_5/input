decl
    int result,factorial(int n);
enddecl

int factorial(int n){
    decl
        int f;
    enddecl
    begin
        write("CALL") ;
        f = 0 ;
        write(n) ;
        write(f) ;
        if( n==1 || n==0 ) then
            f = 1;
        else
            f = n * 1; 
        endif;
        write(f) ;
        write("RET") ;
        return f;
    end
}

int main()
{
    decl
        int a;
    enddecl
    begin
        read(a);
        write(a);
        result = factorial(a);
        write(result);
        breakpoint;
        return 1;
    end
}